<MudDialog>
    <DialogContent>
        El valor ingresado es mayor o igual a la meta ingresada!<br/>
        Por favor done menos de la meta.
    </DialogContent>
    <DialogActions>
        <MudButton Color="Color.Primary" OnClick="Submit">Ok</MudButton>
    </DialogActions>
</MudDialog>


@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }

    void Submit() => MudDialog.Close(DialogResult.Ok(true));
}